vagrantfile:
    vm:
        provider:
            local:
                machines:
                    vflm_olp0m5ysywcp:
                        id: mes-elus-front1
                        hostname: mes-elus.puppet
                        network:
                            private_network: 192.168.50.50
                            forwarded_port:
                                vflmnfp_zi0z5phrrq1i:
                                    host: '2200'
                                    guest: '22'
                        memory: '1024'
                        cpus: '2'
        synced_folder:
            vflsf_eaxlft0h0gn6:
                source: ../api
                target: /var/www/api
                sync_type: nfs
                rsync:
                    exclude:
                        - .idea/
                        - .vagrant/
                        - .git/
                        - .gitignore
                        - .gitkeep
server:
    packages:
        - htop
        - vim
locale:
    settings:
        timezone: Europe/Paris
cron:
    jobs:
        cj_8w1kav4dbthm:
            name: 'Data import for API'
            user: vagrant
            command: 'bash /vagrant/scripts/import_api_data.sh'
            minute: ''
            hour: '4'
            weekday: ''
            month: ''
            monthday: ''
nginx:
    settings:
        default_vhost: 0
    upstreams:
        nxv_ci3s83oyzwpe:
            name: phpfcgi
            upstream_fail_timeout: 10s
            members:
                - '127.0.0.1:9000'
    vhosts:
        website:
            server_name: mes-elus.local
            server_aliases: mes-elus.local
            www_root: /var/www/website
            locations:
                nxvl_uuf6qk5kf88q:
                    fastcgi: phpfcgi
        api:
            server_name: api.mes-elus.local
            server_aliases: api.mes-elus.local
            www_root: /var/www/api/web
            listen_port: '80'
            client_max_body_size: 1m
            ssl: '0'
            ssl_cert: ''
            ssl_key: ''
            ssl_port: '443'
            ssl_protocols: ''
            ssl_ciphers: ''
            rewrite_to_https: '1'
            spdy: '1'
            locations:
                nxvl_ocb82xstby3j:
                    www_root: ''
                    location: /
                    index_files:
                        - app.php
                    try_files:
                        - $uri
                        - /app.php$is_args$args
                    fastcgi: ''
                    fastcgi_index: ''
                    fastcgi_split_path: ''
                    proxy: ''
                    proxy_redirect: ''
                ##############################
                # Only for dev environnement #
                ##############################
                nxvl_kiqvoidsutyc:
                    www_root: ''
                    location: '~ ^/(app_dev|config)\.php(/|$)'
                    fastcgi: phpfcgi
                    fastcgi_index: ''
                    fastcgi_split_path: '^(.+\.php)(/.*)$'
                    fast_cgi_params_extra:
                        - 'SCRIPT_FILENAME $realpath_root$fastcgi_script_name'
                        - 'DOCUMENT_ROOT $realpath_root'
                        - 'APP_ENV dev'
                        - 'APP_DEBUG true'
                    set:
                        - '$path_info $fastcgi_path_info'
                    proxy: ''
                    proxy_redirect: ''
                ##############################
                ##############################
                nxvl_rzw8qrvpth8a:
                    location: '~ ^/app\.php(/|$)'
                    fastcgi: phpfcgi
                    fastcgi_index: ''
                    fastcgi_split_path: '^(.+\.php)(/.*)$'
                    fast_cgi_params_extra:
                        - 'SCRIPT_FILENAME $realpath_root$fastcgi_script_name'
                        - 'DOCUMENT_ROOT $realpath_root'
                    set:
                        - '$path_info $fastcgi_path_info'
                    proxy: ''
                    proxy_redirect: ''
    proxies: {  }
letsencrypt:
    install: '0'
php:
    modules:
        php:
            - cli
            - intl
            - fpm
            - common
            - readline
            - curl
            - imap
            - gd
            - mbstring
            - memcached
            - mysql
            - xml
    ini:
        date.timezone: Europe/Paris
        short_open_tag: 'off'
        magic_quotes_gpc: 'off'
        register_global: 'off'
        register_globals: 'off'
        session.auto_start: 'off'
xdebug:
    install: '1'
nodejs:
    install: '1'
    npm_packages:
        - grunt-cli
        - grunt
        - bower
mariadb:
    settings:
        root_password: 'root'
    users:
        mariadbnu_42qk053ero9u:
            name: dev
            password: 'dev'
    databases:
        mariadbnd_mvf5lpc4ce39:
            name: symfony
    grants:
        mariadbng_nea0e0z4ghis:
            user: dev
            table: 'symfony.*'
mailhog:
    install: '1'
elastic_search:
    install: '1'
    instances:
        esi_pmn23prxanqp:
            name: elk
